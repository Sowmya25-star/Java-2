import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner scanner=new Scanner(System.in);
      int n=scanner.nextInt();
      for(int i =1;i<=n;i++){
        for(char j='A';j<'A'+n;j++){
        System.out.print(j);
       
      }
   
      System.out.println();
    }
  }
}
