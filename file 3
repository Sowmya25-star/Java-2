import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner scanner=new Scanner(System.in);
      int n=scanner.nextInt();
      char a='A';
      for(int i =1;i<=n;i++){
        for(int j=1;j<=n;j++){
        System.out.print(a);
        a++;
      }
   
      System.out.println();
    }
  }
}
